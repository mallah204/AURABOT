{
  "name": "aurabot",
  "version": "1.0.0",
  "description": "A Facebook Chat Bot",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node index.ts",
    "dev": "ts-node-dev --respawn --transpile-only index.ts",
    "build": "tsc",
    "setup": "node -e \"require('child_process').exec(process.platform === 'win32' ? 'setup.bat' : 'bash setup.sh', (e,o,s)=>{console.log(o);process.exit(s)})\"",
    "restore": "ts-node scripts/restore-backup.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "facebook",
    "chat",
    "bot",
    "typescript"
  ],
  "author": "DongDev",
  "license": "MIT",
  "dependencies": {
    "@dongdev/fca-unofficial": "^3.0.21",
    "adm-zip": "^0.5.16",
    "@google/generative-ai": "^0.21.0",
    "axios": "^1.7.7",
    "canvas": "^2.11.2",
    "dotenv": "^16.4.5",
    "form-data": "^4.0.0",
    "lodash": "^4.17.21",
    "sequelize": "^6.37.7",
    "semver": "^7.6.0",
    "sqlite3": "^5.1.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.5",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.12",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0"
  }
}
